{
	"routes": [
		{
			"get": "/",
			"run": "/controllers/site.index"
		},
		{
			"get": "/doc",
			"run": "/controllers/site.doc"
		},
		{
			"get": "/content/:mask",
			"run": "/controllers/content.expand"
		},
		{
			"get": "/content/:mask/stats",
			"run": "/controllers/content.stats"
		},
		{
			"post": "/content",
			"run": "/controllers/content.shrink",
			"via": [
				"/library/request.verifyHeaders",
				"/library/request.verifyKey"
			]
		},
		{
			"delete": "/content",
			"run": "/controllers/content.delete",
			"via": [
				"/library/request.verifyHeaders",
				"/library/request.verifyKey"
			]
		}
	]
}
