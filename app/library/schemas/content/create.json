{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "data"
    ],
    "properties": {
        "public": {
            "type": "boolean"
        },
        "data": {
            "type": "object",
            "required": [
                "mime",
                "value"
            ],
            "properties": {
                "mime": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                },
                "mask": {
                    "type": "string"
                }
            }
        },
        "meta": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "favicon": {
                    "type": "string"
                }
            }
        }
    }
}